{
    "version": 2,
    "builds": [
        {
            "src": "api/index.py",
            "use": "@vercel/python",
            "config": {
                "runtime": "python3.9"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "headers": {
                "cache-control": "no-cache, no-store, must-revalidate",
                "Access-Control-Allow-Origin": "*"
            },
            "dest": "api/index.py"
        }
    ]
}